{
  "name": "koa-sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "check": "npm run format && npm run lint",
    "dev": "nodemon src/index.js",
    "format": "prettier --write \"**/*.+(js|yml|yaml|json)\"",
    "heroku-postbuild": "npm run migrate:undo:all && npm run migrate && npm run seed:all",
    "lint": "eslint .",
    "sequelize": "cd src; sequelize",
    "start": "node src/index.js",
    "start:prod": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "cd src; sequelize db:migrate",
    "migration:generate": "cd src; sequelize migration:generate",
    "migrate:undo": "cd src; sequelize db:migrate:undo",
    "migrate:undo:all": "cd src; sequelize db:migrate:undo:all",
    "seed:all": "cd src; sequelize db:seed:all",
    "seed:generate": "cd src; sequelize seed:generate",
    "model:generate": "cd src; sequelize model:generate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.3.0",
    "nodemon": "1.19.0",
    "prettier": "1.17.1"
  },
  "dependencies": {
    "@bugsnag/js": "^6.3.2",
    "@bugsnag/plugin-koa": "^6.3.0",
    "@koa/cors": "^3.0.0",
    "@shopify/koa-shopify-auth": "^3.1.36",
    "@shopify/koa-shopify-graphql-proxy": "^3.1.1",
    "dotenv": "^8.0.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^14.3.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "3.0.0",
    "koa-helmet": "4.1.0",
    "koa-jwt": "^3.6.0",
    "koa-logger": "3.2.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^3.5.1",
    "sequelize": "^5.10.2",
    "sequelize-cli": "^5.5.0",
    "sequelize-paginate": "^1.1.6"
  }
}
